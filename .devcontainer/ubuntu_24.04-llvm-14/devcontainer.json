{
  "name": "ubuntu_24.04-llvm-14",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--name",
    "mull-ubuntu_24.04-llvm-14"
  ],
  "features": {
    "ghcr.io/devcontainers-extra/features/fish-apt-get:1": {},
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/sshd:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-community/features/bazel": {}
  },
  "postCreateCommand": "infrastructure/devcontainer_post_create.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-vscode.cpptools-extension-pack",
        "imgildev.vscode-mustache-snippets",
        "github.vscode-github-actions",
        "revng.llvm-ir",
        "rreverser.llvm",
        "bazelbuild.vscode-bazel"
      ]
    }
  }
}
